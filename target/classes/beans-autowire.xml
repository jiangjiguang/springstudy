<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="address" class="com.jiangjg.springstudy.autowire.Address">
              <property name="city" value="shanghai"></property>
              <property name="street" value="wujianglu"></property>
       </bean>

       <bean id="person" class="com.jiangjg.springstudy.autowire.Person" autowire="byName">
       </bean>
</beans>